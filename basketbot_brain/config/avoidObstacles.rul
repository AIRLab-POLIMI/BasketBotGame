(AND (DangerAhead)(ProposedForward)) => (&DEL.SpeedModule ANY) (SpeedModule BACKWARD) (ObstacleDanger TRUE);
(AND (DangerBehind)(ProposedBackward)) => (&DEL.SpeedModule ANY) (SpeedModule FORWARD) (ObstacleDanger TRUE);


(AND (AND (ObstacleFront) (ObstacleFrontLeft)) (ObstacleFrontRight)) => (SpeedModule BACKWARD);
(AND (AND (ObstacleRear) (ObstacleRearLeft)) (ObstacleRearRight)) => (SpeedModule FORWARD);

(AND (ObstacleFrontRight) (ProposedForward)) => (RotSpeed LEFT); 
(AND (ObstacleFrontLeft) (ProposedForward)) => (RotSpeed RIGHT); 

(AND (ObstacleRearRight) (ProposedBackward)) =>(RotSpeed RIGHT); 
(AND (ObstacleRearLeft) (ProposedBackward)) =>  (RotSpeed LEFT); 
